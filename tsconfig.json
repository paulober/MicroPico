{
  "compilerOptions": {
    "module": "node16",
    "target": "ES2022",
    "lib": [
      "ES2022"
    ],
    "sourceMap": false,
    "inlineSourceMap": true,
    "removeComments": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "rootDir": "src",
    "outDir": "dist",
    "strict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node16"
  }
}