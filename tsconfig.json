{
  "compilerOptions": {
    "module": "node16",
    "target": "ES2022",
    "lib": [
      "ES2022"
    ],
    "sourceMap": true,
    "inlineSourceMap": false,
    "noEmit": true,
    "removeComments": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "rootDir": "src",
    "outDir": "out",
    "strict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node16",
    "esModuleInterop": true
  }
}