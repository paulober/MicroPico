{
  "name": "pico-go",
  "displayName": "Pico-Go",
  "description": "Adds a REPL console to the terminal that connects to your Raspberry Pi Pico board. Run single files or synchronize your project files to your board.",
  "version": "1.0.3",
  "publisher": "ChrisWood",
  "repository": "https://github.com/cpwood/Pico-Go",
  "icon": "images/logo.png",
  "license": "SEE LICENSE IN <LICENSE.md>",
  "galleryBanner": {
    "color": "#ffffff",
    "theme": "light"
  },
  "engines": {
    "vscode": "^1.36.0"
  },
  "keywords": [
    "raspberry",
    "pi",
    "pico"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./pymakr",
  "contributes": {
    "commands": [
      {
        "command": "pymakr.connect",
        "title": "Pico-Go > Connect"
      },
      {
        "command": "pymakr.disconnect",
        "title": "Pico-Go > Disconnect"
      },
      {
        "command": "pymakr.upload",
        "title": "Pico-Go > Upload project"
      },
      {
        "command": "pymakr.uploadFile",
        "title": "Pico-Go > Upload current file only"
      },
      {
        "command": "pymakr.download",
        "title": "Pico-Go > Download project"
      },
      {
        "command": "pymakr.run",
        "title": "Pico-Go > Run current file"
      },
      {
        "command": "pymakr.runselection",
        "title": "Pico-Go > Run current selection"
      },
      {
        "command": "pymakr.deleteAllFiles",
        "title": "Pico-Go > Delete all files from board"
      },
      {
        "command": "pymakr.projectSettings",
        "title": "Pico-Go > Project settings"
      },
      {
        "command": "pymakr.globalSettings",
        "title": "Pico-Go > Global settings"
      },
      {
        "command": "pymakr.extra.getVersion",
        "title": "Pico-Go > Extras > Get firmware version"
      },
      {
        "command": "pymakr.extra.getSerial",
        "title": "Pico-Go > Extras > List serial ports"
      },
      {
        "command": "pymakr.help",
        "title": "Pico-Go > Help"
      },
      {
        "command": "pymakr.listCommands",
        "title": "Pico-Go > List all Commands"
      }
    ],
    "keybindings": [
      {
        "command": "",
        "key": "ctrl+e",
        "when": "terminalFocus"
      },
      {
        "command": "pymakr.connect",
        "key": "ctrl+shift+c",
        "mac": "cmd+shift+c"
      },
      {
        "command": "pymakr.upload",
        "key": "ctrl+shift+u",
        "mac": "cmd+shift+u"
      },
      {
        "command": "pymakr.uploadFile",
        "key": "ctrl+shift+f",
        "mac": "ctrl+shift+f"
      },
      {
        "command": "pymakr.run",
        "key": "ctrl+shift+r",
        "mac": "cmd+shift+r"
      },
      {
        "command": "pymakr.runselection",
        "key": "ctrl+shift+enter",
        "mac": "cmd+shift+enter"
      },
      {
        "command": "pymakr.globalSettings",
        "key": "ctrl+shift+g",
        "mac": "cmd+shift+g"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "pymakr.upload",
          "group": "pymakr"
        },
        {
          "command": "pymakr.run",
          "group": "pymakr"
        }
      ],
      "editor/context": [
        {
          "command": "pymakr.connect",
          "group": "pymakr"
        },
        {
          "command": "pymakr.run",
          "group": "pymakr"
        },
        {
          "command": "pymakr.runselection",
          "group": "pymakr"
        },
        {
          "command": "pymakr.upload",
          "group": "pymakr"
        }
      ],
      "editor/title": [
        {
          "command": "pymakr.run",
          "group": "pymakr"
        },
        {
          "command": "pymakr.upload",
          "group": "pymakr"
        }
      ]
    }
  },
  "scripts": {
    "postinstall": "pwsh -nop -f ./scripts/mp-download.ps1 -copyonly",
    "download-native": "pwsh -nop -f ./scripts/mp-download.ps1 -ElectronVersions 9.2.1 -IgnoreNodeVersion",
    "build": "babel src --out-dir lib --source-maps",
    "build-dev": "babel src --out-dir lib --source-maps --watch",
    "test-electron": "pwsh -nop -f ./test-electron/runtest.ps1",
    "package-prep": "npm install vsce -g",
    "package": "vsce package",
    "vscode:prepublish": "npm ci && npm prune"
  },
  "backup-required-to-include-binaries": {
    "postinstall": "pwsh -nop -f ./scripts/mp-download.ps1 -copyonly",
    "vscode:prepublish": "npm ci && npm prune"
  },
  "old-scripts": {
    "test": "mocha test/*-test.js",
    "test-ci": "mocha test/*-test.js -R xunit 2>&1 | tee test/TEST-report.xml"
  },
  "dependencies": {
    "binascii": "0.0.2",
    "bindings": "^1.5.0",
    "commander": "4.0.1",
    "copy-paste": "^1.3.0",
    "crypto-js": "^3.1.9-1",
    "debug": "^4.1.1",
    "electron": "^6.1.2",
    "electron-rebuild": "^1.8.6",
    "element-resize-detector": "1.1.15",
    "lie": "^3.3.0",
    "nan": "2.14.0",
    "ncp": "^2.0.0",
    "node-abi": "^2.19.1",
    "object.assign": "^4.1.0",
    "ora": "^4.0.3",
    "promirepl": "^2.0.1",
    "prompt-list": "^3.2.0",
    "rxjs": "^6.5.3",
    "safe-buffer": "^5.2.0",
    "serialport": "^9.0.1",
    "spdx-correct": "^3.1.0",
    "spdx-exceptions": "^2.2.0",
    "telnet-client": "^1.2.3",
    "typescript": "^3.7.2",
    "utf8": "^3.0.0",
    "xterm": "^4.2.0"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.11",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "6.24.1",
    "chai": "^4.2.0",
    "eslint": "^6.7.1",
    "mocha": "^6.2.2",
    "prebuild-install": "^5.3.3",
    "vscode": "^1.1.36"
  }
}
